{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "plugins": ["react", "prettier"],
  "rules": {
    "react/prefer-stateless-function": 1,
    "react/no-danger": "off"
  },
  "settings": {},
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "jsx": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
